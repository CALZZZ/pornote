{
    "name": "Pronote",
    "description": "L'application pour le meilleur des professeur de NSI.",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "tabs",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/logo_pornote16.png",
            "32": "/images/logo_pornote32.png",
            "48": "/images/logo_pornote48.png",
            "128": "/images/logo_pornote128.png"
        }
    },
    "icons": {
        "16": "/images/logo_pornote16.png",
        "32": "/images/logo_pornote32.png",
        "48": "/images/logo_pornote48.png",
        "128": "/images/logo_pornote128.png"
    }
}

